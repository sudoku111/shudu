
public class Cell {
	
	public static int getX(int z,int x){
		int i;
		if (z == 0 || z == 1 || z == 2)
			i = x;
		else if (z == 3|| z == 4 || z ==5)
			i = x+3;
		else 
			i = x+6;
		return i;
		
	}
	
	public static int getY(int z,int y) {
		int j;
		if (z == 0 || z == 3 || z == 6)
			j = y;
		else if (z == 1 || z == 4 || z == 7)
			j = y + 3;
		else
			j = y + 6;
		return j;
	
	}
	
	

}
